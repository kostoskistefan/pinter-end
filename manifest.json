{
    "name": "PinterEnd",
    "description": "Put an end to Pinterest in Google search",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/search*"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "icons": {
        "16": "/images/logo-16.png",
        "32": "/images/logo-32.png",
        "48": "/images/logo-48.png",
        "128": "/images/logo-128.png"
    }
}